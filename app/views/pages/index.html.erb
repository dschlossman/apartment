	<!-- HERO UNIT -->
	<div id="hero-unit" class="hero-unit">
		<div class="hero-content text-center">
			<div class="container">
				<div class="row">
					<div class="col-lg-7 col-lg-offset-5">
						<div id="slidecaption" class="slide-caption">
							<div class="hero-text">
								<h2 class="hero-heading">CAMPUS VIEW <span class="name"></span></h2>
								<div data-looper="go" data-interval="3000" class="looper slide down">
									<div class="looper-inner">
										<div class="item animated"><p>HISTORIC APARTMENTS</p></div>
									</div>
								</div>
							</div>
						</div>
						<div class="hero-buttons">
							<a href="#about" id="btn-about" class="btn btn-info btn-lg"><i class="fa fa-arrow-circle-o-down"></i> READ MORE</a>
							<a href="#contact" id="btn-contact" class="btn btn-primary btn-lg"><i class="fa fa-phone-square"></i> GET IN TOUCH</a>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- END HERO UNIT -->
	

	<!-- MAIN CONTENT -->
	<div id="main-content" class="main-content">
		
		<!-- ABOUT -->
		<div id="about" class="section about">
			<div class="container">
				<div class="section-heading">
					<h2>ABOUT</h2>
					<p>Modern living in a historic setting</p>
					<hr />
				</div>
				<div class="section-content">
					<p>Campus View Apartments is a community of 13 modern loft-style apartments in Fergus Falls, MN. The building was constructed in the early 1900s and has been historically renovated to bring back the large windows, diamond patterned shingling, and many other unique characteristics of the original building. </p>

					<p>The property features five one-bedroom and eight two-bedroom units situated in a quiet, mature neighborhood surrounded by trees and green space.  These units are furnished with modern appliances including a dishwasher, stove, refrigerator, and microwave.  

					</p>
				</div>
			</div>
		</div>
		<!-- END ABOUT -->

		<!-- FULL-WIDTH PARALLAX -->
		<div id="social-network" class="section full-width-parallax parallax">
			<div class="container">
				<h2>Find us elsewhere in the web</h2>
				<ul class="list-inline social-icon">
					<li class="wow rollIn" data-wow-delay='0.3s'><a href="#" data-toggle="tooltip" data-placement="bottom" title="Facebook"><i class="fa fa-facebook"></i></a></li>
					<li class="wow rollIn" data-wow-delay='0.3s'><a href="#" data-toggle="tooltip" data-placement="bottom" title="Twitter"><i class="fa fa-twitter"></i></a></li>
				</ul>
			</div>
		</div>
		<!-- END FULL-WIDTH PARALLAX -->

		<!-- SERVICES -->
		<div id="services" class="section darker services" >
			<div class="container">
				<div class="section-heading">
					<h2>AMENITIES</h2>
					<p>We want you to be <i>comfortable </i></p>
					<hr />
				</div>
				<div class="section-content">
					<div class="row">
						<div class="col-md-6">
							<div class="service-item">
								<span class="icon-phone service-icon"></span>
								<div class="service-text">
									<h3>On Site Management</h3>
									<p>Takes care of maintenance requests quickly. On call <i>24-7</i></p>
								</div>
							</div>
						</div>
						<div class="col-md-6">
							<div class="service-item">
								<span class="icon-checkmark2 service-icon"></span>
								<div class="service-text">
									<h3>Community Amenities</h3>
									<p>Use the community grill on the patio and the common laundry facilities.</p>
								</div>
							</div>
						</div>
						<div class="col-md-6">
							<div class="service-item">
								<span class="icon-checkmark2 service-icon"></span>
								<div class="service-text">
									<h3>Off-street parking</h3>
									<p>Parking on the East side of the building.</p>
								</div>
							</div>
						</div>
						<div class="col-md-6">
							<div class="service-item">
								<span class="icon-checkmark2 service-icon"></span>
								<div class="service-text">
									<h3>Storage Space</h3>
									<p>Downsizing into a maintenance free apartment and need extra space? Ask about our storage options.</p>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- END SERVICES -->

		<!-- WORKS -->
		<div id="works" class="section works">
			<div class="section-heading">
				<h2>FLOORPLANS</h2>
				<p>Find a unit that fits your living style</p>
				<hr />
			</div>
			<div class="section-content">				
				<ul class="list-inline work-item-filters">
					<li><a class="active" href="#" data-filter="*">ALL</a></li>
					<li><a href="#" data-filter=".design">One Bedroom</a></li>
					<li><a href="#" data-filter=".photography">Two Bedroom</a></li>
					<li><a href="#" data-filter=".ui">Efficiency</a></li>
				</ul>

				<div class="work-item-wrapper">
					<ul class="work-item-list list-col-4">
						<li class="work-item design">
							<img src="../../assets/img/portfolio/work1.jpg" alt="One Bedroom">
							<div class="image-overlay">
								<a href="../../assets/img/portfolio/work1-ori.jpg" class="media-popup" title="One Bedroom">
									<div class="work-item-info">
										<h3>One Bedroom</h3>
										<p>638 - 689 Square Feet</p>
										<i class="fa fa-search-plus"></i>
									</div>
								</a>
							</div>
						</li>
						<li class="work-item photography">
							<img src="../../assets/img/portfolio/work7.jpg" alt="Two Bedroom Apartment">
							<div class="image-overlay">
								<a href="../../assets/img/portfolio/work7-ori.jpg" class="media-popup" title="Two Bedroom">
									<div class="work-item-info">
										<h3>Two Bedroom</h3>
										<p>783 - 846 Square Feet</p>
										<i class="fa fa-search-plus"></i>
									</div>
								</a>
							</div>
						</li>
						<li class="work-item ui design">
							<img src="../../assets/img/portfolio/work6.jpg" alt="Efficiency">
							<div class="image-overlay">
								<a href="../../assets/img/portfolio/work6-ori.jpg" class="media-popup" title="Efficiency">
									<div class="work-item-info">
										<h3>Efficiency</h3>
										<p>414 Square Feet</p>
										<i class="fa fa-search-plus"></i>
									</div>
								</a>
							</div>
						</li>
					</ul>
				</div>
			</div>
		</div>
		<!-- END WORKS -->

		<!-- CONTACT -->
		<div id="contact" class="section contact">
			<div class="section-heading">
				<h2>GET IN TOUCH</h2>
				<p>Now it's time to reserve your place.</p>
				<hr />
			</div>
			<div class="section-content">
				<div class="container">
					<div class="row">
						<div class="col-md-4">
							<div class="contact-info">
								<ul class="list-unstyled">
									<li>
										<i class="fa fa-map-marker pull-left"></i>
										<h3>Address</h3>
										<p>1400 Union Avenue<br />Fergus Falls, MN</p>
									</li>
									<li>
										<i class="fa fa-envelope pull-left"></i>
										<h3>Phone</h3>
										<p>+1 701 2616161</p>
									</li>
									<li>
										<i class="fa fa-user pull-left"></i>
										<h3>Email</h3>
										<p><a href="mailto:hello@yourdomain.com">hello@campusviewff.com</a></p>
									</li>
								</ul>
							</div>
						</div>
						<div class="col-md-8">
							<div class="contact-form">
								<div class="alert"></div>
								<form id="contact-form" method="post" role="form" novalidate>
									<div class="row">
										<div class="col-md-6">
											<div class="form-group">
												<label class="sr-only" for="name">Name:</label>	
												<input type="text" id="name" class="form-control" name="name" placeholder="Name*" required>
											</div>
										</div>
										<div class="col-md-6">
											<div class="form-group">
												<label class="sr-only" for="email">Email:</label>
												<input type="email" id="email" class="form-control" name="email" placeholder="Email*" required>
											</div>
										</div>
									</div>
									<div class="form-group">
										<label class="sr-only" for="subject">Subject:</label>
										<input type="text" id="subject" class="form-control" name="subject" placeholder="Subject">
									</div>
									<div class="form-group">
										<label class="sr-only" for="message">Message:</label>
										<textarea id="message" name="message" class="form-control" placeholder="Message*" required></textarea>
									</div>
									<button type="submit" class="btn btn-primary btn-lg" id="submit-button">
										<i class="fa loading-icon"></i> <span>Send Message</span>
									</button>
									<input type="hidden" name="msg-submitted" id="msg-submitted" value="true"> 
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- END CONTACT -->

		<!-- TWITTER STREAM -->
		<div id="twitter-stream" class="section twitter-stream">
			<div class="container">
				<div class="twitter-icon">
					<i class="fa fa-twitter"></i>
				</div>
				<div id="tweet"></div>
			</div>
		</div>
		<!-- TWITTER STREAM -->

	</div>
	<!-- END MAIN CONTENT -->

